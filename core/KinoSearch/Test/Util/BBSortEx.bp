parcel KinoSearch cnick Kino;

/** SortExternal for ByteBufs.
 */

class KinoSearch::Test::Util::BBSortEx 
    extends KinoSearch::Util::SortExternal {

    VArray   *external;
    u32_t     external_tick;

    inert BBSortEx*
    new(u32_t mem_thresh = 0x1000000, VArray *external = NULL);

    inert BBSortEx*
    init(BBSortEx *self, u32_t mem_thresh = 0x1000000, 
        VArray *external = NULL);

    void
    Feed(BBSortEx *self, decremented Obj *item);

    void
    Flush(BBSortEx *self);

    void
    Flip(BBSortEx *self);

    int
    Compare(BBSortEx *self, Obj **a, Obj **b);

    incremented Obj* 
    Recover_Item(BBSortEx *self);

    public void
    Destroy(BBSortEx *self);
}


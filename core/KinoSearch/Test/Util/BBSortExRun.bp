parcel KinoSearch cnick Kino;

/** SortExRun for sorting ByteBufs.
 */

class KinoSearch::Test::Util::BBSortExRun 
    extends KinoSearch::Util::SortExRun {

    VArray   *external;
    u32_t     external_tick;
    u32_t     mem_thresh;
    u32_t     mem_consumed;

    inert incremented BBSortExRun* 
    new(VArray *external);

    inert BBSortExRun*
    init(BBSortExRun *self, VArray *external);

    void 
    Set_Mem_Thresh(BBSortExRun *self, u32_t mem_thresh);

    int
    Compare(BBSortExRun *self, Obj **a, Obj **b);

    Obj* 
    Read_Elem(BBSortExRun *self);

    void
    Destroy(BBSortExRun *self);
}

